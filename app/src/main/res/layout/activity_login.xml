<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.corn_farmer.src.kakao.LoginActivity">

    <ImageView
        android:id="@+id/login_logo_iv"
        android:layout_width="147dp"
        android:layout_height="92dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="105dp"
        android:src="@drawable/cornfarmer_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/login_none_login_bt"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/login_none_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_logo_iv" />

    <ImageView
        android:id="@+id/login_line_iv"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/login_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/login_none_login_bt" />

    <Button
        android:id="@+id/login_kakao_login_bt"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/login_kakao_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_line_iv" />

    <Button
        android:id="@+id/login_naver_login_bt"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/login_naver_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_kakao_login_bt" />


</androidx.constraintlayout.widget.ConstraintLayout>